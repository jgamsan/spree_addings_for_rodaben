<%= render :partial => 'spree/admin/shared/tires_setting_menu' %>
<h1><%= t(:nueva_carga_masiva_de_imagenes) %></h1>
<%= form_tag update_file_admin_tire_settings_path, :html => {:multipart => true} do %>
  <div data-hook="admin_load_massive_images_form_fields">
    <table data-hook="name">
      <tr>
        <td><%= label_tag :Archivo, "Archivo a importar" %>:</td>
        <td><%= file_field_tag :image, {'style' => 'width:100px'} %></td>
      </tr>
    </table>
  </div>
  <%= submit_tag "Submit" %>
<% end %>
