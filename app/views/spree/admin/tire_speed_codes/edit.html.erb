<h1><%= t(:editing_tire_speed_code) %></h1>
<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @speed_code } %>
<%= form_for [:admin, @speed_code] do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p class="form-buttons" data-hook="buttons">
    <%= button t(:update) %>
    <%= t(:or) %> <%= link_to t(:cancel), admin_tire_speed_codes_path, :id => 'cancel_link' %>
  </p>
<% end %>
