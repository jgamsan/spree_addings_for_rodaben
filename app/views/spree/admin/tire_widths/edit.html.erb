<h1><%= t(:editing_tire_width) %></h1>
<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @width } %>
<%= form_for [:admin, @width] do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p class="form-buttons" data-hook="buttons">
    <%= button t(:update) %>
    <%= t(:or) %> <%= link_to t(:cancel), admin_tire_widths_path, :id => 'cancel_link' %>
  </p>
<% end %>
